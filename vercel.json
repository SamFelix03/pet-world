{
  "rewrites": [
    {
      "source": "/api/imagegen/:path*",
      "destination": "https://imagegen-739298578243.us-central1.run.app/:path*"
    },
    {
      "source": "/api/clipgen/:path*",
      "destination": "https://clipgen-739298578243.us-central1.run.app/:path*"
    },
    {
      "source": "/api/s3-proxy",
      "destination": "/api/s3-proxy"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}

